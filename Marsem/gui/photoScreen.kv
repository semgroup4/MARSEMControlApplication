<PhotoScreen>:
    name: 'PhotoScreen'
    CustomLayout:
        Button:
            text: 'View Photos'
            size: 170, 50
            size_hint: None,None
            height: '48dp'
            pos: root.width-self.width-485, root.height-self.height*2+self.height-74
            on_release: root.showImages()
        FileChooserIconView:
            pos: root.width-self.width, root.height-self.height*1.7+self.height/2
            size: root.size
            on_selection: root.loadImage(self)
            # Change picture type by changing the filter.
            filters: ['*.jpg']
            path: root.getPath()